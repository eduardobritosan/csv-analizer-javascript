<!doctype html>

<html>
  <head>

  <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <title><%- title %></title>
	<script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/underscore.js/1.8.2/underscore-min.js"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.10.2/jquery.min.js"></script>
	<link href="public/global.css" rel="stylesheet" type="text/css">
	<script type="text/javascript" src="public/csv.js"></script>
	  <script type="text/html" id="tableTemplate">
		<p>
			<table class="center" id="result">
				<% _.each(rows, function(row) { %>
				<tr class="<%=row.type%>">
					<% _.each(row.items, function(name) { %>
					<td><%= name %></td>
					<% }); %>
				</tr>
				<% }); %>
			</table>
		</p>
	</script>

  </head>
  <body>
    <%- body %>
  </body>
</html>
